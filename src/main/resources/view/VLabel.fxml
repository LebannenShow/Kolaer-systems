<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<fx:root style="-fx-background-color: transparent;" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Button fx:id="labelRun" alignment="CENTER" maxHeight="150.0" maxWidth="150.0" mnemonicParsing="false" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER">
         <graphic>
            <BorderPane maxHeight="200.0" maxWidth="200.0" minHeight="50.0" minWidth="50.0">
               <bottom>
                  <Label fx:id="labelName" alignment="TOP_CENTER" maxHeight="100.0" maxWidth="100.0" text="Label" textAlignment="CENTER" wrapText="true" BorderPane.alignment="CENTER" />
               </bottom>
               <center>
                  <ImageView fx:id="labelIcon" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" BorderPane.alignment="CENTER" />
               </center>
            </BorderPane>
         </graphic>
      </Button>
   </center>
</fx:root>
